  "version": "0.2.0",
  "configurations": [
    {
      "name": "starter",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file", "lib/env/.dev.json"]
    },
    {
      "name": "starter (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file", "lib/env/.dev.json"]
    },
    {
      "name": "starter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file", "lib/env/.prod.json"]
    }
  ]
}
